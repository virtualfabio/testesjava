package course.sessao3;

import java.util.Scanner;

public class Excercicio3 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		System.out.println("Informe o nome do aluno: ");
		Student aluno1 = new Student();
		aluno1.nome = sc.nextLine();
		aluno1.notaTrimestre1 = sc.nextDouble();
		aluno1.notaTrimestre2 = sc.nextDouble();
		aluno1.notaTrimestre3 = sc.nextDouble();
		
		sc.close();
	}

}
